{
	"$schema": "https://railway.app/railway.schema.json",
	"build": {
		"builder": "DOCKERFILE",
		"buildCommand": "yarn build",
		"dockerfilePath": "Dockerfile"
	},
	"deploy": {
		"numReplicas": 1,
		"startCommand": "npm start",
		"healthcheckPath": "/",
		"sleepApplication": false,
		"restartPolicyType": "ALWAYS"
	}
}